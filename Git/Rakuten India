package com.epam.interview;

public class Example {


    int [] array = {1,2,3,5};
    
    int arraySize = array.length + 1;
    
    //Missing no logic
    
    int sumAll = (arraySize * (arraySize+1) )/2;
    
    int sum = 0
    
    for (int i=0; i<=arraySize-2; i++) {
        sum = sum+array[i];
    }
    

    int missingNumber = sumAll - sum ;
    
    

}

